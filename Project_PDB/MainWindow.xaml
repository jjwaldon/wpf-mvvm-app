<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="Project" x:Class="Project_PDB.MainWindow"
        mc:Ignorable="d"
        Title="Project_BPD" Height="450" Width="800">
    <Grid x:Name="Grid_Default">
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button x:Name="Filter" Content="Filtering" Click="BtnClick1" MinWidth="100" Width="286" />
            <Button x:Name="Modify" Content="Modifying" Click="BtnClick2" MinWidth="100" Width="253" />
            <Button x:Name="Modify_1_n" Content="Modify 1-n" MinWidth="100" Width="262" Click="BtnClick3" />
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0" NavigationUIVisibility="Hidden" Background="#FFB9D2F5" />
    </Grid>
</Window>
